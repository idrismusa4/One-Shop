{
  "expo": {
    "name": "oneshop",
    "slug": "oneshop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.harry654.oneshop"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow oneshop to access the microphone",
          "speechRecognitionPermission": "Allow oneshop to securely recognize user speech"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4e9f2d6b-3dca-40a2-9467-7a80d3346908"
      }
    }
  }
}
